{
  "name": "clothx",
  "version": "0.0.0",
  "engines": {
    "npm": "6.4.1",
    "node": "8.11.3"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "set_production_environment": "firebase use clothxnet --non-interactive --token $FIREBASE_TOKEN ",
    "set_development_environment": "firebase use clothxtest --non-interactive --token $FIREBASE_TOKEN ",
    "build_prod_env": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build  --configuration=production",
    "build_test_env": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --configuration=test",
    "deploy_to_gitlab_page": "mkdir public && cp -a /dist/store-website/. /public/",
    "deploy_to_firebase": "firebase deploy --non-interactive --token $FIREBASE_TOKEN",
    "deploy_to_prod": "firebase deploy --only hosting:spoteasy --non-interactive --token $FIREBASE_TOKEN",
    "deploy_to_dev": "firebase deploy --only hosting:spoteasytest --non-interactive --token $FIREBASE_TOKEN"
  },
  "private": true,
  "dependencies": {
